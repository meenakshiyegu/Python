path=-1
n=int(raw_input())
a=map(int,raw_input().split())
for i in range(0,n):
    count=0
    j=i
    while(True):
        count+=1
        if(a[j]==-1):
            break
        elif(i==a[j]):
            if(count>path):
                path=count
            break
        else:
            temp=j
            j=a[j]
            a[temp]=-1 
print path
